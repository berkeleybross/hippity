{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Install NPM packages",
      "group": "build",
      "type": "npm",
      "script": "install",
      "problemMatcher": []
    },
    {
      "label": "Commit test",
      "group": "build",
      "type": "npm",
      "script": "build",
      "problemMatcher": ["$eslint-stylish", "$tsc"]
    },
    {
      "label": "Lint ts",
      "group": "build",
      "type": "npm",
      "script": "lint:ts",
      "problemMatcher": ["$tsc"]
    },
    {
      "label": "Lint es",
      "group": "build",
      "type": "npm",
      "script": "lint:es",
      "problemMatcher": ["$eslint-stylish"]
    }
  ]
}
