{
  "name": "@berkeleybross/restclient",
  "version": "0.1.1",
  "description": "Simple middleware based REST client",
  "main": "dist/@berkeleybross/restclient.common.js",
  "web": "dist/@berkeleybross/restclient.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production BUILD=production npm-run-all clean -p build:* lint test --aggregate-output",
    "build:es": "cross-env BABEL_ENV=es babel -d es -D --ignore '*.test.js' src/",
    "build:js": "rollup -c scripts/rollup.config.js",
    "build:esm": "rollup -c scripts/rollup.config.esm.js",
    "clean": "rimraf dist/* es/*",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test BUILD=production jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/berkeleybross/restclient.js.git"
  },
  "author": "berkeleybross",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/berkeleybross/restclient.js/issues"
  },
  "homepage": "https://github.com/berkeleybross/restclient.js#readme",
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.1.5",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^22.0.6",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.0.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
